  <div class="container-fluid">
    <div class="backing">
      <div class="col-md-3"></div>
      <div class="col-md-6 profile">
        <table border="1" class="show-table">
            <p id="notice"><%= notice %></p>
            <%= form_for @bar do |f| %>
          <tr>
            <td class="key"><strong class="titles">Name: </strong></td>
            <td><%= f.text_field :name, class: "form-color" %></td>
          </tr>
          <tr>
            <td><strong class="titles">Street: </strong></td>
            <td><%= f.text_field :street, class: "form-color" %></td>
          </tr>
          <tr>
            <td><strong class="titles">City: </strong></td>
            <td><%= f.text_field :city_state_zip, class: "form-color" %> </strong></td>
          </tr>
          <tr>
            <td class="bluebuttons" class="titles"><span class="selects">Day: </span></td>
            <td class="bluebuttons"><%= f.select :day, [['Monday'],['Tuesday'],['Wednesday'],['Thursday'],['Friday'],['Saturday'],['Sunday']], class: "form-color" %></td>
          </tr>
          <tr>
            <td><strong class="titles">Time: </strong></td>
            <td><%= f.text_field :time, class: "form-color" %></td>
          </tr>
          <tr>
            <td class="titles"><span class="selects">Theme: </span></td>
            <td class="bluebuttons"><%= f.select :theme, [['general'],['entertainment'],['sports']] %></td>
          <tr>
            <td><strong class="titles">Web: </strong></td>
            <td><%= f.text_field :web, class: "form-color" %></td>
          </tr>
          <tr>
            <td><strong class="titles">Latitude: </strong></td>
            <td><%= f.text_field :lat, class: "form-color" %></td>
          </tr>
          <tr>
            <td><strong class="titles">Longitude: </strong></td>
            <td><%= f.text_field :lon, class: "form-color" %></td>
          </tr>
          <tr>
            <td><%= f.submit 'Add', :class => "button" %></td>
          </tr>
            
          <% end %>
        </table>

      <div class="col-md-3"></div>
    </div>
  </div>
